     RowBox[{
      SuperscriptBox["2", "2"], "+", 
      SuperscriptBox["3", "2"], "+", 
      SuperscriptBox["5", "2"], "+", 
      SuperscriptBox["7", "2"], "+", 
      SuperscriptBox["11", "2"], "+", 
      SuperscriptBox["13", "2"], "+", 
      SuperscriptBox["17", "2"]}]}]}], TraditionalForm]]],
 " somehow means ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"x", "=", "1"}], "36"], 
     RowBox[{"t", "[", 

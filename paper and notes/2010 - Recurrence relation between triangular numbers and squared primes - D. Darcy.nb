    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["2", "2"], " ", "+", " ", 
       SuperscriptBox["3", "2"], " ", "+", " ", 
       SuperscriptBox["5", "2"], " ", "+"}], " ", "..."}], " ", "+", 
     SuperscriptBox[
      SubscriptBox["p", "z"], "2"]}]}], TraditionalForm]]],
